<h1>New account</h1>
<%= form_for User.new do |f| %>
  <%= f.email_field :email, placeholder: 'email', require: true %>
  <%= f.password_field :password, placeholder: 'Password', require: true %>
  <%= f.password_field :password_confirmation, placeholder: 'Password confirmation', require: true %>
  <%= f.submit 'Register' %>
<% end %>
